{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "store:allow-load",
    "store:allow-set",
    "store:allow-get",
    "store:allow-delete",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "**/ScreentimeTracker/screenshots"
        },
        {
          "path": "$APPDATA/file"
        }
      ]
    }
  ]
}
